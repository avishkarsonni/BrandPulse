# Environment Configuration for Sentiment Analysis Dashboard

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_API_KEY=your_api_key_here
REACT_APP_API_TIMEOUT=10000

# Application Settings
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=false
REACT_APP_VERSION=1.0.0

# Features Flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_EXPORT=true

# Analytics
REACT_APP_ANALYTICS_ID=your_analytics_id
REACT_APP_ERROR_TRACKING=false

# Social Media Integration (if applicable)
REACT_APP_TWITTER_API_KEY=your_twitter_api_key
REACT_APP_FACEBOOK_API_KEY=your_facebook_api_key
REACT_APP_INSTAGRAM_API_KEY=your_instagram_api_key
